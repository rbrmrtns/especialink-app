{
  "expo": {
    "name": "especialink-front",
    "slug": "especialink-front",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.robrmartins.especialinkfront"
    },
    "plugins": [
      "expo-font",
      [
        "@rnmapbox/maps/app.plugin.js",
        {
          "RNMapboxMapsVersion": "10.16.0"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.8.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "36e80eae-8abc-4ee1-946f-a115d1c9e65d"
      }
    }
  }
}
